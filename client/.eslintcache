, [hours]);\n\n  return (\n    <li className={styles.weeklyItem}>\n      <div className={styles.date}>\n        <div className={styles.weekday}>{moment(day).format('ddd')}</div>\n        <div>{moment(day).format('DD')}</div>\n      </div>\n      <div className={`${styles.date} ${displayHours > 600 ? styles.red : styles.green}`}>\n        {\n          displayHours === 0\n          ?\n          null\n          :\n          minToH(displayHours)\n        }\n      </div>\n      {moment(day).format('YY-MM-DD') ===\n      moment(new Date()).format('YY-MM-DD') ? (\n        <Disabled label='Start' width=\"5rem\" />\n      ) : null}\n    </li>\n  );\n};\n\nexport default WeeklyItem;\n","/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/hooks/useTime.js",["315"],"// NPM PACKAGES\nimport moment from 'moment';\n\nmoment.updateLocale('en', {\n  week: {\n    dow : 1, // Monday is the first day of the week.\n  }\n});\n\nexport const getAge = date => {\n  const currentDate = moment(new Date());\n  let age;\n\n  if (currentDate.diff(date, 'weeks') < 1) {\n    age = currentDate.diff(date, 'days') + ' day(s) old';\n  } else if (currentDate.diff(date, 'months') < 1) {\n    age = currentDate.diff(date, 'weeks') + ' week(s) old';\n  } else if (currentDate.diff(date, 'years') < 1) {\n    age = currentDate.diff(date, 'months') + ' month(s) old';\n  } else if (currentDate.diff(date, 'years') > 1) {\n    age = currentDate.diff(date, 'years') + ' year(s) old';\n  };\n\n  return age;\n};\n\nexport const convertHours = time => {\n  const hoursMinutes = time.split(/[.:]/);\n  const hours = parseInt(hoursMinutes[0], 10);\n  const minutes = hoursMinutes[1] ? parseInt(hoursMinutes[1], 10) : 0;\n  return (hours + minutes / 60). toFixed(2);\n};\n\nexport const minToH = time => {\n  const h = (time / 60);\n  const roundedH = Math.floor(h);\n  const min = (h - roundedH) * 60;\n  const roundedMin = ('0' + Math.round(min)).slice(-2);\n  return `${roundedH}:${roundedMin}`;\n}\n\nexport const minToHandMin = time => {\n  const h = (time / 60);\n  const roundedH = Math.floor(h);\n  const min = (h - roundedH) * 60;\n  const roundedMin = ('0' + Math.round(min)).slice(-2);\n  return `${roundedH}h ${roundedMin}min`;\n}\n\nexport const duration = (start, end) => {\n  const timeDifference = minToH(moment(start).diff(end, \"minutes\"));\n  return timeDifference;\n}\n","/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/components/Button/Close.js",[],"/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/components/ProgressRing/index.js",[],"/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/components/List/Resources/ResourceItem.js",[],"/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/components/DatePicker/Cell.js",[],"/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/components/Button/Disabled.js",[],"/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/components/Input/index.js",[],"/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/pages/auth/register/Step2.js",["316"],"/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/pages/auth/register/Step1.js",[],"/Users/mymac/Bootcamp/Week-21/APIA_Project/client/src/pages/auth/register/Step3.js",[],{"ruleId":"317","replacedBy":"318"},{"ruleId":"319","replacedBy":"320"},{"ruleId":"321","severity":1,"message":"322","line":25,"column":16,"nodeType":"323","messageId":"324","endLine":25,"endColumn":23},{"ruleId":"321","severity":1,"message":"325","line":25,"column":25,"nodeType":"323","messageId":"324","endLine":25,"endColumn":33},{"ruleId":"321","severity":1,"message":"326","line":26,"column":9,"nodeType":"323","messageId":"324","endLine":26,"endColumn":16},{"ruleId":"321","severity":1,"message":"327","line":19,"column":21,"nodeType":"323","messageId":"324","endLine":19,"endColumn":33},{"ruleId":"321","severity":1,"message":"328","line":19,"column":35,"nodeType":"323","messageId":"324","endLine":19,"endColumn":41},{"ruleId":"321","severity":1,"message":"329","line":10,"column":8,"nodeType":"323","messageId":"324